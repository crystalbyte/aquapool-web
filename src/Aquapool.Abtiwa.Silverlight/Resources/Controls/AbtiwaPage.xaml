            </Grid>
        </Border>
        <Border Grid.Column="3"
                Grid.ColumnSpan="3"
                Background="#FF85BF50"
                BorderBrush="#3EE8E8E8"
                BorderThickness="0,0,2,0">
            <TextBlock Width="Auto"
                       Margin="5,0,0,0"
                       HorizontalAlignment="Stretch"
                       VerticalAlignment="Center"
                       
                       FontSize="10"
                       FontStretch="Normal"
                       FontWeight="Thin"
                       Foreground="#FFFFFFFF"
                       TextWrapping="Wrap">
                <Run Text="Professionelle" /><Run Text=" Planung und Realisierung. Alles von Anfang an" /><Run Text="!" />
            </TextBlock>
        </Border>
        <Border Grid.Row="4"
                Grid.Column="1"
                Grid.ColumnSpan="5"
                BorderBrush="#3EE8E8E8"
                BorderThickness="2,0,2,0">
            <Grid x:Name="TextLayer"
                  Width="Auto"
                  Height="Auto"
                  Background="#CAFFFFFF">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="0.039*" />
                    <ColumnDefinition Width="0.01*" />
                    <ColumnDefinition Width="0.733*" />
                    <ColumnDefinition Width="0.139*" />
                    <ColumnDefinition Width="0.079*" />
                    <ColumnDefinition Width="0*" />
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="0.017*" />
                    <RowDefinition Height="0.008*" />
                    <RowDefinition Height="0.033*" />
                    <RowDefinition Height="0.205*" />
                    <RowDefinition Height="0.017*" />
                    <RowDefinition Height="0.032*" />
                    <RowDefinition Height="0.646*" />
                    <RowDefinition Height="0.042*" />
                </Grid.RowDefinitions>
                <controls:Viewbox x:Name="ViewBoxFirstText"
                                  Grid.Row="1"
                                  Grid.RowSpan="3"
                                  Grid.Column="2"
                                  Grid.ColumnSpan="2">
                    <TextBlock x:Name="TextBlockFirstText"
                               FontSize="6.5"
                               TextAlignment="Left"><Run 
                             FontSize="7.5"
                             Foreground="#FF0088B7"
                             Text=" VERKEHRSWASSER- UND FLUSSBAU, GRUND- UND SPEZIALTIEFBAU, BETON-" />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF0088B7"
                             Text="UND STAHLBETONBAU, BRÜCKENBAU" />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="Als Fachbüro für konstruktiven Verkehrswasserbau mit der Spezialisierung" />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="für schwimmende und feste Steganlagen, Hafen- und Kaianlagen," />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="schwimmende Häuser, Slipanlagen, Ufer – und Sohlsicherungen, sowie," />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="Spezialtief-, Beton-, Stahlbeton- und Brückenbau sind wir uneingeschränktgut" />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="ausgebildete und erfahrene Dipl.-Ingenieure und Techniker, die überwiegend" />
                        <!--  <Run Text=" "/>  -->
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="kontinuierlich in den o.g. Bereichen aller Leistungsphasen der HOAI im Bereich" />
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="der Objekt- und Tragwerksplanung sowie Bauleitung und Projektsteuerung" />
                        <LineBreak />
                        <Run 
                             FontSize="7.5"
                             Foreground="#FF4D4D4D"
                             Text="seit über 20 Jahren arbeiten." />
                    </TextBlock>
                </controls:Viewbox>
                <controls:Viewbox x:Name="Group_Copy"
                                  Grid.Row="2"
                                  Grid.RowSpan="1"
                                  Canvas.Left="0"
                                  Canvas.Top="1.52588e-005"
                                  Width="Auto"
                                  Height="Auto"
                                  Margin="0,0,0,0">
                    <Canvas Width="20" Height="20">
                        <Path x:Name="Path1"
                              Canvas.Left="0"
                              Canvas.Top="0"
                              Width="20"
                              Height="19.9912"
                              Data="F1 M 20,19.9912L 0,19.9912L 0,0L 20,0L 20,19.9912"
                              Fill="#FF589124"
                              Stretch="Fill" />
