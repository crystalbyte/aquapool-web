                    </LinearGradientBrush>
                </Grid.Background>
                <Image Margin="0,0,0,0"
                       IsHitTestVisible="True"
                       Source="../Images/angle.png"
                       Stretch="Fill" />
            </Grid>
        </Border>
        <Border Grid.Column="1" BorderThickness="2,0,0,0">
            <Grid Width="20" Height="20">
                <controls:Viewbox x:Name="Group"
                                  Canvas.Left="0"
                                  Canvas.Top="1.52588e-005"
