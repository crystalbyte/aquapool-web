                <controls:Viewbox x:Name="ViewBoxSecondText"
                                  Grid.Row="5"
                                  Grid.RowSpan="2"
                                  Grid.Column="2"
                                  Grid.ColumnSpan="1"
                                  Margin="0,0,1,0">
                    <TextBlock x:Name="TextBlockSecondText"
                               Canvas.Left="0"
                               Canvas.Top="0"
                               Width="Auto"
                               Height="Auto"
                               FontSize="2.5"
                               RenderTransformOrigin="0.5,0.5"
                               TextAlignment="Left">
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF0088B7"
                             Text="WIR PLANEN, BERECHNEN UND REALISIEREN FÜR SIE" />
                        <LineBreak />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="VERKEHRSWASSERBAU" />
                        <LineBreak />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  feste und schwimmende Steganlagen aus allen Materialien" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  schwimmende Häuser" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Hafen- und Kaianlagen für Binnen- und Seehäfen" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Slipanlagen und Bootsanlegestellen für sämtliche Schiffsgrößen" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Abweisedalben und Leitwerke für Brücken und Kaianlagen" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Ufer- und Sohlsicherungen" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Uferwände und Hochwasserschutzanlagen" />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="•  Bootshausgründungen" />
                        <LineBreak />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
                             Foreground="#FF4D4D4D"
                             Text="FLUSSBAU" />
                        <LineBreak />
                        <LineBreak />
                        <Run 
                             FontSize="6.5"
