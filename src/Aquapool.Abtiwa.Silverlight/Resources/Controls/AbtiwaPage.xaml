                        <Path x:Name="Path"
                              Canvas.Left="0"
                              Canvas.Top="0"
                              Width="20"
                              Height="19.9912"
                              Data="F1 M 20,19.9912L 0,19.9912L 0,0L 20,0L 20,19.9912"
                              Fill="#FF589124"
                              Stretch="Fill" />
                        <controls:Viewbox x:Name="Group_0"
                                          Canvas.Left="0"
                                          Canvas.Top="0"
                                          Width="20"
                                          Height="19.9912">
                            <Canvas Width="20" Height="19.9912">
                                <Canvas x:Name="Clip"
                                        Width="800"
                                        Height="599.736"
                                        Clip="F1 M 20,19.9912L 0,19.9912L 0,0L 20,0L 20,19.9912">
                                    <Path x:Name="Path_1"
                                          Canvas.Left="0"
                                          Canvas.Top="0"
                                          Width="20"
                                          Height="19.9912"
                                          Data="F1 M 19.9732,0L 19.9732,0C 9.06183,0 0.335785,8.74988 0,19.5831L 0,19.9912L 20,19.9912L 19.9732,0"
