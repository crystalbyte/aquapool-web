                Grid.Column="1"
                Grid.ColumnSpan="5"
                Background="#FFFFFFFF"
                BorderBrush="#3EE8E8E8"
                BorderThickness="2,0,2,0">
            <Grid x:Name="HeaderImageLayer"
                  Width="Auto"
                  Height="Auto">
                <Grid.Background>
                    <LinearGradientBrush StartPoint="0.5,0.999998" EndPoint="0.5,2.99692e-006">
