             xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows">
    <Application.Resources>
        <!--  Resources scoped at the Application level should be defined here.  -->
        <LinearGradientBrush x:Key="HeaderGradient" StartPoint="0.273088,0.191294" EndPoint="0.273088,0.554757">
            <GradientStop Offset="0.0136986" Color="#AFFFFFFF" />
            <GradientStop Offset="0.251142" Color="#91E1EDF6" />
            <GradientStop Offset="0.584475" Color="#73C4DBED" />
            <GradientStop Offset="0.850557" Color="#3AE1EDF6" />
            <GradientStop Offset="1" Color="#00FFFFFF" />
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="HeaderGradient2" StartPoint="0.53671,1.24487" EndPoint="0.53671,-0.0151306">
            <GradientStop Offset="0.0456621" Color="#00FFFFFF" />
