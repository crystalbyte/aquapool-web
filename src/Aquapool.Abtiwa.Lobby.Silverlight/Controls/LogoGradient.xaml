<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Aquapool.Lobby.LogoGradient"
	d:DesignWidth="769" d:DesignHeight="108">

	<Grid x:Name="LayoutRoot">
		<Grid.Background>
		<LinearGradientBrush StartPoint="0.516111,1.27426" EndPoint="0.516111,0.244264">
					<LinearGradientBrush.GradientStops>
						<GradientStop Color="#FFFFFFFF" Offset="0"/>
						<GradientStop Color="#00FFFFFF" Offset="0.00561798"/>
						<GradientStop Color="#61E9EEF0" Offset="0.538136"/>
						<GradientStop Color="#C2D3DEE2" Offset="1"/>
